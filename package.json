{
  "name": "next-redux-jest-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node tests/commands/postRun.js && next dev",
    "test": "node tests/commands/preRun.js && jest --config=jest.config.js || node tests/commands/postRun.js",
    "proxy": "node tests/commands/postRun.js node next-proxy.js",
    "test-coverage": "node tests/commands/preRun.js && jest --config=tests/jest.config.js --coverage || node tests/commands/postRun.js",
    "export": "node tests/commands/postRun.js && next export",
    "build": "node tests/commands/postRun.js && next build",
    "start": "node tests/commands/postRun.js && next start",
    "storybook": "node tests/commands/postRun.js && start-storybook -p 9001 -c .storybook",
    "storybook-build": "node tests/commands/postRun.js && build-storybook"
  },
  "dependencies": {
    "@testing-library/react": "^11.2.2",
    "express": "^4.17.1",
    "next": "10.0.3",
    "path": "^0.12.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.2",
    "react-saga": "^0.3.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "tape-async": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.8",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "fetch-mock": "^9.11.0",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "redux-mock-store": "^1.5.4",
    "shelljs": "^0.8.4",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "chromatic": "^5.8.0",
    "tape": "^5.0.1",
    "http-proxy-middleware": "^1.0.6"
  }
}
