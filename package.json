{
  "name": "next-redux-jest-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node tests/commands/postRun.ts && next dev",
    "test": "node tests/commands/preRun.ts && jest --config=tests/jest.config.js || node tests/commands/postRun.ts",
    "test-coverage": "node tests/commands/preRun.ts && jest --config=tests/jest.config.js --coverage || node tests/commands/postRun.ts",
    "build": "node tests/commands/postRun.ts && next build",
    "start": "node tests/commands/postRun.ts && next start",
    "export": "node tests/commands/postRun.ts && next export",
    "storybook": "node tests/commands/postRun.ts && start-storybook -p 9001 -c .storybook",
    "storybook-build": "node tests/commands/postRun.ts && build-storybook"
  },
  "dependencies": {
    "express": "^4.17.1",
    "next": "^10.0.6",
    "path": "^0.12.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-redux": "^7.1.16",
    "@types/styled-components": "^5.1.7",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "http-proxy-middleware": "^1.0.6",
    "jest": "^26.6.3",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.3",
    "redux-mock-store": "^1.5.4",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "chromatic": "^5.8.0"
  }
}
